{% extends 'base.html.twig' %}

{% block title 'Mon compte' %}

{% block body %}

    <div class="container">

        <h1 class="pt-3">Mon compte</h1>
        {% if is_granted('ROLE_COMPANY') %}
            {% include 'user/company/_profile_company.html.twig' %}
        {% else %}
            {% include 'user/candidate/_profile_candidate.html.twig' %}
        {% endif %}
    </div>
{% endblock %}

{% block javascript %}
    {{ parent() }}
    {% if is_granted('ROLE_CANDIDATE') %}
        {{ encore_entry_script_tags('candidate_graphic') }}
    {% endif %}
{% endblock %}

{% extends 'base.html.twig' %}

{% block title 'Métier' %}

{% block body %}
    <div class="container pt-5">
        <h2 class="text-center">Renseigner votre métier rechercher.</h2>

        {{ form_start(form) }}
        {{ form_row(form.contracts) }}
        {{ form_row(form.jobCategory) }}
        <div id="form_search_job">
            {{ form_row(form.job) }}
        </div>
        <button class="btn btn-primary">Envoyer</button>
        {{ form_end(form) }}
    </div>
{% endblock %}

